-buildpath: \
	osgi.annotation;version=6.0.1,\
	osgi.core;version=6.0,\
	osgi.cmpn;version=6.0,\
	example.api;version=latest

-testpath: \
	${junit}

javac.source: 1.8
javac.target: 1.8

Bundle-Version: 0.0.0.${tstamp}

# OSGi Compendium 133.3.2
Require-Capability: osgi.extender; \
	filter:="(&(osgi.extender=osgi.serviceloader.processor)(version>=1.0)(!(version>=2.0)))", \
	osgi.serviceloader; \
	filter:="(osgi.serviceloader=example.api.ExampleService)"

Export-Package: example.consumer